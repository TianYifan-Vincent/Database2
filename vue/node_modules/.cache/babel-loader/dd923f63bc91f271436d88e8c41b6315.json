AC,QAAL,CAAc;AACZ,YAAA,IAAI,EAAE,OADM;AAEZ,YAAA,OAAO,EAAE,GAAG,CAAC;AAFD,WAAd;AAIF;AACD,OAfD;AAgBJ,WAAK,oBAAL,GAA4B,KAA5B;AACD,KAzDM;AA0DP,IAAA,SA1DO,uBA0DK;AAAA;;AACV,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,UAAjB;AACA,WAAK,KAAL,CAAW,QAAX,EAAqB,QAArB,CAA8B,UAAC,KAAD,EAAW;AACvC,YAAI,KAAJ,EAAW;AACT,UAAA,OAAO,CAAC,IAAR,CAAa,SAAb,EAAuB,MAAI,CAAC,UAA5B,EAAwC,IAAxC,CAA6C,UAAA,GAAE,EAAK;AAClD,YAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;;AACA,gBAAI,GAAG,CAAC,IAAJ,KAAa,GAAjB,EAAsB;AACpB,cAAA,MAAI,CAAC,QAAL,CAAc;AACZ,gBAAA,IAAI,EAAE,SADM;AAEZ,gBAAA,OAAO,EAAE;AAFG,eAAd,EADoB,CAKpB;;;AACA,cAAA,MAAI,CAAC,MAAL;AACD,aAPD,MAOM,IAAG,GAAG,CAAC,IAAJ,KAAW,CAAd,EAAgB;AACpB,cAAA,MAAI,CAAC,KAAL,CAAW,QAAX,EAAqB,WAArB;;AACA,cAAA,MAAI,CAAC,QAAL,CAAc;AACZ,gBAAA,IAAI,EAAE,OADM;AAEZ,gBAAA,OAAO,EAAE,GAAG,CAAC;AAFD,eAAd;AAIF,aANM,MAOD;AACH,cAAA,MAAI,CAAC,KAAL,CAAW,QAAX,EAAqB,WAArB;;AACA,cAAA,MAAI,CAAC,QAAL,CAAc;AACZ,gBAAA,IAAI,EAAE,OADM;AAEZ,gBAAA,OAAO,EAAE,GAAG,CAAC;AAFD,eAAd;AAIF;AACD,WAvBD;AAwBD;AAAC,OA1BJ;AA2BA,WAAK,oBAAL,GAA4B,KAA5B;AACD,KAxFM;AA0FX;AACI,IAAA,aA3FO,yBA2FO,QA3FP,EA2FiB;AACtB,WAAK,KAAL,CAAW,QAAX,EAAqB,WAArB;AACA,WAAK,oBAAL,GAA4B,KAA5B,CAFsB,CAEa;AACpC,KA9FM;AA+FP,IAAA,aA/FO,yBA+FO,QA/FP,EA+FiB;AACtB,WAAK,KAAL,CAAW,QAAX,EAAqB,WAArB;AACA,WAAK,oBAAL,GAA4B,KAA5B,CAFsB,CAEa;AACpC,KAlGM;AAmGP,IAAA,aAnGO,yBAmGO,QAnGP,EAmGiB;AACtB,WAAK,KAAL,CAAW,QAAX,EAAqB,WAArB;AACA,WAAK,oBAAL,GAA4B,KAA5B,CAFsB,CAEa;AACpC,KAtGM;AAuGX;AACI,IAAA,qBAxGO,iCAwGe,IAxGf,EAwGqB;AAC1B,WAAK,KAAL,CAAW,SAAX,EAAsB,WAAtB;AACA,MAAA,IAAI,GAFsB,CAEnB;AACR,KA3GM;AA4GP,IAAA,sBA5GO,kCA4GgB,IA5GhB,EA4GsB;AAC3B,WAAK,KAAL,CAAW,QAAX,EAAqB,WAArB;AACA,MAAA,IAAI,GAFuB,CAEpB;AACR,KA/GM;AAgHP,IAAA,sBAhHO,kCAgHgB,IAhHhB,EAgHsB;AAC3B,WAAK,KAAL,CAAW,QAAX,EAAqB,WAArB;AACA,MAAA,IAAI,GAFuB,CAEpB;AACR;AAnHM,GA7CI;AAmKb,EAAA,QAAQ,EAAC;AACP,IAAA,cADO,4BACS;AACd,aAAO,KAAK,KAAL,CAAW,KAAX,CAAiB,YAAxB;AACD,KAHM;AAIP,IAAA,aAJO,2BAIQ;AACb,aAAO,KAAK,KAAL,CAAW,KAAX,CAAiB,IAAjB,CAAsB,QAA7B;AACF;AANO,GAnKI;AA2Kb,EAAA,OA3Ka,qBA2KH;AACR,SAAK,YAAL,GAAkB,KAAK,cAAvB;AACA,SAAK,UAAL,CAAgB,QAAhB,GAAyB,KAAK,aAA9B;AACA,SAAK,WAAL,CAAiB,QAAjB,GAA0B,KAAK,aAA/B;AACA,SAAK,IAAL,GAJQ,CAKR;AACA;AACF;AAlLa,CAAf","sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n    <!--    功能区-->\r\n    <div style=\"margin:10px 0\">\r\n      <el-button type=\"primary\" @click=\"centerDialogVisible2=true\" style=\"font-size: large\">登记信息</el-button>\r\n      <el-button type=\"primary\" @click=\"centerDialogVisible1 = true\" style=\"font-size: large\" >新增车辆</el-button>\r\n      <el-dialog v-model=\"centerDialogVisible1\" title=\"车辆信息\" width=\"30%\" center :before-close=\"closeExpertFormDialog\">\r\n      <el-form ref=\"Vehicle\" :model=\"formVehicle\" label-width=\"120px\" >\r\n        <el-form-item label=\"车架号\" prop=\"vin\">\r\n          <el-input v-model=\"formVehicle.vin\" placeholder=\"请输入车架号\" style=\"width: 70%\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"车牌号\" prop=\"license\">\r\n          <el-input v-model=\"formVehicle.license\" placeholder=\"请输入车牌号\" style=\"width: 70%\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"颜色\" prop=\"color\">\r\n          <el-input v-model=\"formVehicle.color\" placeholder=\"请输入车辆颜色\" style=\"width: 70%\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"车型\" prop=\"class\">\r\n          <el-input v-model=\"formVehicle.class\" placeholder=\"请输入车型\" style=\"width: 70%\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"车辆类别\" prop=\"type\">\r\n          <el-select v-model=\"formVehicle.type\" placeholder=\"请输入车辆类别\" style=\"width: 70%\">\r\n            <el-option label=\"微型车\" value=\"微型车\" />\r\n            <el-option label=\"中型车\" value=\"中型车\" />\r\n            <el-option label=\"大型车\" value=\"大型车\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item >\r\n          <el-button type=\"primary\" @click=\"onSubmit\">确认新增</el-button>\r\n          <el-button type=\"primary\" @click=\"cancelExpert1('Vehicle')\" style=\"margin-left: 40px\">取消</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n      </el-dialog>\r\n      <el-dialog v-model=\"centerDialogVisible2\" title=\"基本信息\" width=\"30%\" center :before-close=\"closeExpertFormDialog2\">\r\n        <el-form ref=\"Client\" :model=\"formClient\" label-width=\"120px\" >\r\n          <el-form-item label=\"名称\" prop=\"name\">\r\n            <el-input v-model=\"formClient.name\" placeholder=\"请输入用户名称\" style=\"width: 70%\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"性质\" prop=\"nature\">\r\n            <el-select v-model=\"formClient.nature\" placeholder=\"请输入用户性质\" style=\"width: 70%\">\r\n              <el-option label=\"个人\" value=\"个人\" />\r\n              <el-option label=\"单位\" value=\"单位\" />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"联系人\" prop=\"contact\">\r\n            <el-input v-model=\"formClient.contact\" placeholder=\"请输入联系人\" style=\"width: 70%\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"联系电话\" prop=\"phone\">\r\n            <el-input v-model=\"formClient.phone\" placeholder=\"请输入联系电话\" style=\"width: 70%\"/>\r\n          </el-form-item>\r\n          <el-form-item >\r\n            <el-button type=\"primary\" @click=\"onSubmit2\">确认</el-button>\r\n            <el-button type=\"primary\" @click=\"cancelExpert2('Client')\" style=\"margin-left: 40px\">取消</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-dialog>\r\n      <el-dialog v-model=\"centerDialogVisible3\" title=\"新增委托\" width=\"30%\" center :before-close=\"closeExpertFormDialog3\">\r\n        <el-form ref=\"Repair\" :model=\"formRepair\" label-width=\"120px \" style=\"margin-left: -40px\">\r\n          <el-form-item label=\"车架号\" prop=\"vin\">\r\n            <el-input v-model=\"formRepair.vin\" placeholder=\"请输入车架号\" style=\"width: 50%\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"结算方式\" prop=\"resource\">\r\n            <el-radio-group v-model=\"formRepair.resource\">\r\n              <el-radio label=\"自付\" />\r\n              <el-radio label=\"索赔\" />\r\n              <el-radio label=\"三包\" />\r\n            </el-radio-group>\r\n          </el-form-item>\r\n          <el-form-item label=\"进厂时间\" prop=\"approach_time\" >\r\n            <el-date-picker\r\n                v-model=\"formRepair.approach_time\"\r\n                type=\"datetime\"\r\n                placeholder=\"Select date and time\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item label=\"粗略故障\" prop=\"failure\">\r\n            <el-input v-model=\"formRepair.failure\" type=\"textarea\" placeholder=\"请粗略描述您车辆的故障（字数在255以下）\"/>\r\n          </el-form-item>\r\n          <el-form-item label=\"进厂油量\" prop=\"fuel\">\r\n            <el-input v-model=\"formRepair.fuel\" placeholder=\"进厂时车辆所剩油量\" style=\"width: 50%\"/><span>%</span>\r\n          </el-form-item>\r\n          <el-form-item label=\"进厂里程\" prop=\"mile\">\r\n            <el-input v-model=\"formRepair.mile\" placeholder=\"进厂时车辆里程数\" style=\"width: 50%\"/><span>km</span>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" @click=\"onSubmit3\">确认新增</el-button>\r\n            <el-button type=\"primary\" @click=\"cancelExpert3('Repair')\" style=\"margin-left: 40px\">取消</el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </el-dialog>\r\n    </div>\r\n    <div v-for=\"(item,index) in formVehicles\" :key=\"item\">\r\n      <div v-if=\"item.license\">\r\n        <el-descriptions\r\n            class=\"margin-top\"\r\n            title=\"车辆信息\"\r\n            :column=\"1\"\r\n            size=large\r\n            border\r\n            style=\"width: 60%;font-size: large\"\r\n        >\r\n          <template #extra>\r\n            <el-button type=\"primary\" @click=\"centerDialogVisible3 = true\" style=\"font-size: large;background-color: #001b7a\">新增维修委托</el-button>\r\n            <el-button type=\"primary\" style=\"font-size: large;background-color: #001b7a\" @click=\"$router.push('/onway')\">历史维修委托</el-button>\r\n          </template>\r\n          <el-descriptions-item>\r\n            <template #label>\r\n              <div class=\"cell-item\">\r\n                <el-icon :style=\"iconStyle\">\r\n                  <user />\r\n                </el-icon>\r\n                车牌号\r\n              </div>\r\n            </template>\r\n            {{item.license}}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template #label>\r\n              <div class=\"cell-item\">\r\n                <el-icon :style=\"iconStyle\">\r\n                  <iphone />\r\n                </el-icon>\r\n                车架号\r\n              </div>\r\n            </template>\r\n            {{item.vin}}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template #label>\r\n              <div class=\"cell-item\">\r\n                <el-icon :style=\"iconStyle\">\r\n                  <location />\r\n                </el-icon>\r\n                颜色\r\n              </div>\r\n            </template>\r\n            {{item.color}}\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template #label>\r\n              <div class=\"cell-item\">\r\n                <el-icon :style=\"iconStyle\">\r\n                  <tickets />\r\n                </el-icon>\r\n                车型\r\n              </div>\r\n            </template>\r\n            <el-tag size=\"large\" style=\"font-size: large\">{{item.category}}</el-tag>\r\n          </el-descriptions-item>\r\n          <el-descriptions-item>\r\n            <template #label>\r\n              <div class=\"cell-item\">\r\n                <el-icon :style=\"iconStyle\">\r\n                  <office-building />\r\n                </el-icon>\r\n                车辆类别\r\n              </div>\r\n            </template>\r\n            {{item.type}}\r\n          </el-descriptions-item>\r\n        </el-descriptions>\r\n        </div>\r\n        <div v-else>暂无车辆信息</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref ,computed} from 'vue'\r\nimport {useStore} from \"vuex\";\r\nimport request from \"@/util/request\";\r\nimport ClientLayout from \"@/layout/ClientLayout\";\r\nimport ClientAside from \"@/components/ClientAside\";\r\nexport default {\r\n  name: \"Client\",\r\n  inject:['reload'],\r\n  components:{\r\n    ClientLayout,\r\n    ClientAside\r\n  },\r\n  setup() {\r\n    const centerDialogVisible1 = ref(false)\r\n    const centerDialogVisible2 = ref(false)\r\n    const centerDialogVisible3 = ref(false)\r\n    const size = ref('')\r\n    const value1 = ref('')\r\n    const store = useStore()\r\n    const iconStyle = computed(() => {\r\n      const marginMap = {\r\n        large: '8px',\r\n        default: '6px',\r\n        small: '4px',\r\n      }\r\n      return {\r\n        marginRight: marginMap[size.value] || marginMap.default,\r\n      }\r\n    })\r\n    return {\r\n      centerDialogVisible1,\r\n      centerDialogVisible2,\r\n      centerDialogVisible3,\r\n      size,\r\n      iconStyle,\r\n      store,\r\n      value1\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      formVehicles:[],\r\n      formVehicle: {\r\n      },\r\n      formClient: {\r\n      },\r\n      formRepair: {\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    load(){\r\n      request.get(`/client/${this.formClient.clientId}`).then(res => {\r\n        this.formVehicles = res.data//数组类的数据\r\n      })\r\n    },\r\n    onSubmit() {\r\n      this.$refs['Vehicle'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/client\",this.formVehicle).then(res => {\r\n            console.log(res)\r\n            if (res.code === 454) {\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"新增车辆成功\"\r\n              })\r\n              // this.store.commit('submitClientInfo', this.formClient)//同步提交个人信息数据\r\n              this.load()\r\n              this.reload()//重新加载组件\r\n            }else if(res.code===2){\r\n              this.$refs[\"Vehicle\"].resetFields();\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n            else {\r\n              this.$refs[\"Vehicle\"].resetFields();\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n        }})\r\n      this.load()\r\n      this.centerDialogVisible1 = false\r\n      // this.formVehicle = JSON.parse(JSON.stringify(this.defaultForm2))\r\n    },\r\n    onSubmit2() {\r\n          request.put(\"/client\",this.formClient).then(res => {\r\n            console.log(res)\r\n            if (res.code === 200) {\r\n              this.$message({\r\n                type: \"success\",\r\n                message: res.msg\r\n              })\r\n              this.store.commit('submitClientInfo', this.formClient)//同步提交个人信息数据\r\n              this.reload()\r\n            } else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n      this.centerDialogVisible2 = false\r\n    },\r\n    onSubmit3() {\r\n      console.log(this.formRepair)\r\n      this.$refs['Repair'].validate((valid) => {\r\n        if (valid) {\r\n          request.post(\"/client\",this.formRepair).then(res => {\r\n            console.log(res)\r\n            if (res.code === 454) {\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"新增委托成功\"\r\n              })\r\n              // this.store.commit('submitClientInfo', this.formClient)//同步提交个人信息数据\r\n              this.reload()\r\n            }else if(res.code===2){\r\n              this.$refs[\"Repair\"].resetFields();\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n            else {\r\n              this.$refs[\"Repair\"].resetFields();\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n        }})\r\n      this.centerDialogVisible3 = false\r\n    },\r\n\r\n//取消dialog\r\n    cancelExpert1(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.centerDialogVisible1 = false; //关闭对话框\r\n    },\r\n    cancelExpert2(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.centerDialogVisible2 = false; //关闭对话框\r\n    },\r\n    cancelExpert3(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.centerDialogVisible3 = false; //关闭对话框\r\n    },\r\n//关闭dialog前\r\n    closeExpertFormDialog(done) {\r\n      this.$refs['Vehicle'].resetFields();\r\n      done();//done 用于关闭 Dialog\r\n    },\r\n    closeExpertFormDialog2(done) {\r\n      this.$refs['Client'].resetFields();\r\n      done();//done 用于关闭 Dialog\r\n    },\r\n    closeExpertFormDialog3(done) {\r\n      this.$refs['Repair'].resetFields();\r\n      done();//done 用于关闭 Dialog\r\n    },\r\n\r\n  },\r\n  computed:{\r\n    getvehicleform(){\r\n      return this.store.state.vehicle_form\r\n    },\r\n    getclientform(){\r\n      return this.store.state.form.clientId\r\n    }\r\n  },\r\n  created() {\r\n    this.formVehicles=this.getvehicleform\r\n    this.formClient.clientId=this.getclientform\r\n    this.formVehicle.clientId=this.getclientform\r\n    this.load()\r\n    // this.load()\r\n    // this.reload()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.cell-item {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.margin-top {\r\n  margin-top: 20px;\r\n}\r\n</style>"],"sourceRoot":""}]}