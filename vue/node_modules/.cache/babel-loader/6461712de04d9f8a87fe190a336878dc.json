      if (valid) {\n          request.get(\"/user/login\",this.form).then(res => {\n            console.log(res)\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"登陆成功\"\n              })\n              this.$router.push(\"/home\")  //登陆成功后进行页面跳转，跳转到主页\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              })\n            }\n          })\n        }\n      })\n    },\n    register(){\n      this.$router.replace('/register')\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":""}]}