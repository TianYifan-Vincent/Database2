\n  data(){\r\n    return{\r\n      defaultOpenedsArray:['1','2'],\r\n      form: {},\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur'},\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur'},\r\n        ],\r\n        confirm: [\r\n          { required: true, message: '请再次输入密码', trigger: 'blur'},\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n\r\n  },\r\n  computed:{\r\n    getform(){\r\n      return this.store.state.form\r\n    }\r\n  },\r\n  created() {\r\n    this.form=this.getform\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .div1{\r\n    margin-left: -20px;\r\n  }\r\n</style>"]}]}